label poemresponse_start:
    $ poemsread = 0
    $ skip_transition = False
    label poemresponse_loop:
        $ skip_poem = False
        if renpy.music.get_playing() and not (renpy.music.get_playing() == audio.t5 or renpy.music.get_playing() == audio.t5c):
            $ renpy.music.play(audio.t5, fadeout=1.0, if_changed=True)
        if skip_transition:
            scene bg club_day
        else:
            scene bg club_day
            with wipeleft_scene
        $ skip_transition = False
        if not renpy.music.get_playing():
            play music t5
    label poemresponse_start2:
        $ skip_poem = False
        if persistent.playthrough == 2:
            $ pt = "2"
        else:
            $ pt = ""
        if poemsread == 0:
            $ menutext = "Кому показать своё стихотворение в первую очередь?"
        else:
            $ menutext = "Кто увидит моё стихотворение следующим?"

        menu:
            "[menutext]"

            "Саёри" if not s_readpoem and persistent.playthrough == 0:
                $ s_readpoem = True
                if chapter == 1 and poemsread == 0:
                    "Думаю, проще всего будет сначала показать его Саёри."
                    "Всё-таки мы близкие друзья."
                call poemresponse_sayori from _call_poemresponse_sayori
            "Нацуки" if not n_readpoem:
                $ n_readpoem = True
                if chapter == 1 and poemsread == 0:
                    "Вчера я сказал Нацуки, что мне интересны её стихи."
                    "Наверное, будет честно сначала показать свой ей."
                call poemresponse_natsuki from _call_poemresponse_natsuki
            "Юри" if not y_readpoem and not y_ranaway:
                $ y_readpoem = True
                if chapter == 1 and poemsread == 0:
                    "Юри выглядит самой опытной в этом деле, так что с неё и начну."
                    "Откровенно говоря, её мнению я могу доверять."
                call poemresponse_yuri from _call_poemresponse_yuri
            "Моника" if not m_readpoem:
                $ m_readpoem = True
                if chapter == 1 and poemsread == 0:
                    "Начну с Моники."
                    "Вчера мне показалось, что она прямо-таки жаждет увидеть мои стихи, поэтому хотелось бы ей продемонстрировать, что я стараюсь."
                call poemresponse_monika from _call_poemresponse_monika
        $ poemsread += 1
        if poemsread < 3 or (persistent.playthrough == 0 and poemsread < 4):
            jump poemresponse_loop


    $ s_readpoem = False
    $ n_readpoem = False
    $ y_readpoem = False
    $ m_readpoem = False
    $ poemsread = 0
    return

label poemresponse_sayori:
    scene bg club_day
    show sayori 1a zorder 2 at t11
    with wipeleft_scene
    $ poemopinion = "med"
    if s_poemappeal[chapter - 1] < 0:
        $ poemopinion = "bad"
    elif s_poemappeal[chapter - 1] > 0:
        $ poemopinion = "good"
    $ nextscene = "ch" + pt + str(chapter) + "_s_" + poemopinion
    call expression nextscene from _call_expression
    if not skip_poem:
        $ nextscene = "ch" + pt + str(chapter) + "_s_end"
        call expression nextscene from _call_expression_1
    return

label poemresponse_natsuki:
    scene bg club_day
    show natsuki 1c zorder 2 at t11
    with wipeleft_scene
    $ poemopinion = "med"
    if n_poemappeal[chapter - 1] < 0:
        $ poemopinion = "bad"
    elif n_poemappeal[chapter - 1] > 0:
        $ poemopinion = "good"
    $ nextscene = "ch" + pt + str(chapter) + "_n_" + poemopinion
    call expression nextscene from _call_expression_2
    if not skip_poem:
        $ nextscene = "ch" + pt + str(chapter) + "_n_end"
        call expression nextscene from _call_expression_3
    return

label poemresponse_yuri:
    scene bg club_day
    show yuri 1a zorder 2 at t11
    with wipeleft_scene
    $ poemopinion = "med"
    if y_poemappeal[chapter - 1] < 0:
        $ poemopinion = "bad"
    elif y_poemappeal[chapter - 1] > 0:
        $ poemopinion = "good"
    $ nextscene = "ch" + pt + str(chapter) + "_y_" + poemopinion
    call expression nextscene from _call_expression_4
    if not skip_poem:
        $ nextscene = "ch" + pt + str(chapter) + "_y_end"
        call expression nextscene from _call_expression_5
    return

label poemresponse_monika:
    scene bg club_day
    show monika 1a zorder 2 at t11
    with wipeleft_scene
    if m_poemappeal[chapter - 1] < 0:
        $ poemopinion = "bad"
    elif m_poemappeal[chapter - 1] > 0:
        $ poemopinion = "good"
    $ nextscene = "ch" + pt + str(chapter) + "_m_start"
    call expression nextscene from _call_expression_6
    if not skip_poem:
        $ nextscene = "ch" + pt + str(chapter) + "_m_end"
        call expression nextscene from _call_expression_7
    return

label ch1_y_end:
    call showpoem (poem_y1, img="yuri 3t") from _call_showpoem
    y 3t "..."
    y "И... Извини, у меня такой ужасный почерк!"
    mc "Что?"
    mc "Мне так не показалось..."
    y 2v "Но ты так долго читал..."
    mc "А..."
    mc "Ну, мне редко приходится читать рукописные тексты, так что..."
    mc "И вообще, я считаю, что у тебя прелестный почерк."
    y 2t "Да?"
    y 2u "Какое... облегчение..."
    mc "К тому же мне понравился стих."
    mc "Хоть он и довольно короткий, но всё равно крайне описательный."
    y 2t "Не слишком коротко вышло?"
    y "Обычно я пишу куда длиннее..."
    mc "Да не особо."
    y 1m "Мне... действительно приятно, что тебе понравилось."
    y "Буду честна..."
    y 1a "Поскольку это наш первый обмен, я решила написать что-нибудь нейтральное."
    y "Что-то, что легко воспринимается, я бы сказала."
    mc "Тебе нравятся призраки, Юри?"
    y 1m "Хи-хи."
    y "На самом деле здесь совсем не о призраках, [player]."
    mc "Серьёзно?"
    mc "Похоже, я вообще не уловил сути..."
    y 1u "Полагаю, это из-за того, что ты просто пробежался глазами..."
    y "Но запомни: поэты в большинстве случаев выплёскивают на бумагу свои собственные мысли, чувства и переживания."
    y 1a "Причём это обычно не прямой рассказ или описание."
    y "В данном контексте стихотворный персонаж сравнивается с призраком лишь символически."
    y 2l "Льнущим к последнему оплоту спокойствия, неспособным отпустить прошлое."
    y "Чтобы вскоре остаться ни с чем..."
    mc "...В этом ключе всё выглядит намного мрачнее."
    mc "Я бы о таком даже не подумал..."
    mc "Впечатляет."
    if poemopinion == "good":
        y 2f "Что?"
        y 3v "Н-Нет-нет, это ерунда!"
        y "Твой тоже впечатляет, так что..."
        mc "Не-а..."
        mc "Мне бы, наверное, есть чему у тебя поучиться."
        y 4a "...Ты так думаешь?"
        mc "Да, безусловно."
        y "Ой..."
        y 2s "Знаешь..."
        y "Я так переживала обо всём этом."
        y "Но в конечном итоге мне понравилось."
        y "Я буду и дальше стараться для тебя, [player]."
        mc "Эм..."
        mc "И я."
    else:
        y 1u "Да ладно, ерунда..."
        y "Ну... я очень рада, что ты так думаешь."
        y 1a "Не забывай, что пройдёт не так много времени, и ты тоже научишься быстро смекать подобные вещи."
        mc "Да, наверное, ты права."
        mc "Думаю, мне просто стоит не бросать на полпути."
        y "Рассчитываю на тебя."
    return

label ch2_y_end:
    call showpoem (poem_y2) from _call_showpoem_1
    y 2m "Эм..."
    y "Этот получился немного смелее, чем вчерашний..."
    mc "Заметно."
    mc "Намного более метафорично..."
    "Может, дело во мне, но я никак не могу додуматься, о чем этот стих."
    y 1a "Это верно."
    y "Он немного ближе к моему основному стилю..."
    y "Использовать стих как основу для излияния ярких образов и передачи эмоций через них."
    mc "Да, если воспринимать всё буквально, я даже не смогу представить, о чём здесь говорится..."
    y 2f "Ну..."
    y "Я думаю, что это зависит от личного восприятия."
    y "Я хотела выразить чувства, которые испытываю, предаваясь своим необычным увлечениям..."
    y 2v "Эмоции подобного рода мне обычно приходится держать при себе."
    y "Так что иногда мне нравится писать о них."
    if n_readpoem and (n_poemappeal[0] >= 0 or n_poemappeal[1] >= 0):
        mc "Ха, забавно..."
        y 2e "?.."
        mc "Разве Нацуки пишет не о том же?"
        mc "О том, как сторонятся людей со странными интересами."
        y 2h "Что?"
        y "Она... она пишет о таком?"
        mc "Да..."
        mc "Она говорила о том, что неважно, чем ты увлекаешься — до тех пор, пока ты никому не вредишь."
        y 3r "Она... Она права!"
        y 3o "Ой... В смысле..."
        y "Она действительно так считает?.."
        mc "Да."
        mc "Кажется, у вас это общая черта..."
        y 3h "Это... ну, это интересно..."
        y "Она всегда казалась мне человеком, который будет смеяться над моими пристрастиями..."
        y "Но, выходит, я сама виновата в том, что судила предвзято, да?.."
        y 3p "Ой... Пожалуйста, не говори ей об этом!"
        mc "Аха-ха. Не переживай, мне незачем."
        y 1l "Ладно..."
        y 1a "Спасибо, что показал мне свой стих."
    else:
        mc "Почему ты их сдерживаешь?"
        y 3v "По... Потому что..."
        y "Это стыдно..."
        y "И люди будут надо мной смеяться."
        y "У тебя нет ничего подобного, [player]?"
        mc "Ну..."
        mc "Вроде бы есть..."
        y 2h "Мне кажется, у каждого имеется подобная мелочь."
        y "Всё, что нам остается, это уважать других и их личные предпочтения."
        y "Даже если иногда это сложно, даже если некоторые вещи нам непривычны..."
    y 1a "В конце концов, если бы я не научилась запирать в себе свои странности, скорее всего, я бы себя ненавидела."
    y 2u "Ч-что-то я слишком разошлась..."
    y "...Но я рада, что ты хороший слушатель."
    if y_appeal >= 2:
        y 2s "Ты много чего умеешь..."
        y "Писать, слушать..."
        y 2u "Таких, как ты, очень мало, [player]..."
        mc "Х-Хоть я немного преувеличиваю..."
        y 2v "Но это... мои собственные чувства."
        y "Никогда не думала, что мне будет так приятно делиться с кем-то своим творчеством..."
        y 2s "Но теперь я вроде как с нетерпением жду этих моментов..."
        y 2m "Такое... такое приятное чувство."
        y "И я тебе за него благодарна."
        mc "Да... да не за что, собственно..."
        "Юри искренне улыбнулась мне."
        "Всего на мгновение её робость испарилась."
    return
label ch3_y_end:
    if y_appeal >= 3:
        jump ch3_y_end_special
    call showpoem (poem_y3, img="yuri 2v") from _call_showpoem_2
    y "Эм..."
    y "Я осознаю, что пляж это довольно бессмысленная тема для стихотворения."
    y "Но я сделала всё, что в моих силах, чтобы метафорически это обыграть."
    if not n_readpoem or n_appeal >= 3:
        mc "Судя по твоим словам, ты вообще не хотела о нём писать..."
        y 2e "О, а ты не слышал?.."
        y 2h "После вчерашнего мы с Нацуки... ну..."
        y "Было... так невероятно, что мы описали похожие явления столь различными способами."
        y "Поэтому Нацуки предложила ещё раз написать на общую тему."
        if n_readpoem:
            mc "Ясно..."
            "Что-то мне подсказывает, что Нацуки не собирается делиться с остальными тем стихом, что показала мне..."
            "Естественно, я не стал говорить об этом Юри."
    else:
        mc "Да, Нацуки рассказывала мне об этом."
        y 3t "У-уже?.."
        y "Она ведь не говорила ничего странного, да?"
        y "Она просто предложила ещё раз написать на общую тему..."
    y 2f "Полагаю, ради того, чтобы удобнее было искать различия в наших авторских стилях... или мыслительных процессах."
    y 2w "В общем, это была её идея!.."
    y "Зная её, не удивительно, что это пришло ей в голову."
    y "Наверное, просто хочет повыпендриваться."
    y 2v "Не сказать, чтобы у меня был практический интерес к её стилю..."
    y "Я просто последовала её просьбе."
    y "Но..."
    y 1s "Ну, мне кажется, не так уж плохо время от времени писать о чём-нибудь незамысловатом."
    y 1m "Освежает, знаешь?"
    y "Мне совсем не помешает изредка давать мозгам отдых."
    mc "Да... пожалуй, соглашусь."
    mc "Спасибо, что показала свой стих."
    return
label ch3_y_end_special:
    call showpoem (poem_y3b, img="yuri 4b") from _call_showpoem_3
    "Закончив читать стих, я протянул его обратно Юри."
    "Но вместо того, чтобы забрать его, она отвернулась."
    y "..."
    y "Тебе... не понравилось?"
    mc "А... нет, что ты."
    mc "Просто я... не особо понимаю, как должен реагировать."
    "Несмотря на то, что обычно стихи Юри были загадочными, на сей раз было несложно догадаться, о чём он."
    if n_readpoem:
        "К тому же, это явно не тот стих, о котором говорила Нацуки..."
        "...В том смысле, что, скорее всего, я единственный, кому Юри показала этот."
    y 2v "Н-не знаю, получится ли у меня объяснить, о чём это стихотворение..."
    mc "Не нужно."
    mc "Это я понял."
    y 4c "..."
    "Слова давались Юри ещё тяжелее, чем обычно."
    mc "Он... многое для тебя значит?"
    "Юри кивнула."
    mc "Я не умею красиво говорить, но..."
    mc "Я рад, что ты поделилась им со мной."
    mc "Так что спасибо тебе."
    mc "И, надеюсь, мы и дальше будем проводить время вместе."
    show yuri 4e
    "Несмотря на моё неумение устанавливать зрительный контакт, я уловил несмелую улыбку, появившуюся на губах Юри."
    "И попробовал вернуть ей стих ещё раз."
    show yuri 4a
    "Но вместо этого Юри ласково взяла мои руки и отодвинула обратно ко мне."
    "Я растерялся от её тёплого прикосновения."
    y 1v "Ты можешь..."
    y "Эм-м..."
    y "Стих, он..."
    "Юри в очередной раз стала запинаться, неспособная закончить фразу."
    mc "Ты хочешь сказать, что я могу оставить его себе?"
    "Юри кивнула."
    mc "С удовольствием."
    show yuri 1u
    "Юри снова едва уловимо улыбнулась, как будто не хотела, чтобы я это заметил."
    y "С тобой всегда..."
    y "С тобой всегда... так приятно."
    y "Знаю, у меня не очень хорошо с общением, но..."
    y "Надеюсь... что когда-нибудь смогу тебе отплатить."
    mc "Ага."
    mc "Не переживай."
    mc "Мне кажется, ты хорошо справляешься."
    "Юри наконец повернулась лицом ко мне."
    y 1s "Думаю... нам лучше поторопиться, пока Моника ничего не сказала."
    y "Но уверена, мы сможем поговорить позже..."
    mc "Да."
    mc "Я тоже так думаю."
    "Юри робко улыбнулась мне, и я вернулся на своё место, чтобы спрять её стих."
    return

label ch1_n_end:
    call showpoem (poem_n1, img="natsuki 2s") from _call_showpoem_4
    n 2q "Ага..."
    n "Я же говорила, что тебе не понравится."
    mc "Мне нравится."
    n 2h "Чего?"
    n "Не ври!"
    mc "Даже не думал."
    mc "Почему ты так уверена, что мне не должно было понравиться?"
    n 5w "Ну..."
    n "Потому что!"
    n "Все в старшей школе уверены, что авторский стиль должен быть изысканным и всё такое..."
    n 5q "Поэтому народ даже не воспринимает мои стихи всерьёз."
    mc "Но разве их смысл не в том, чтобы самовыражаться?"
    mc "Стиль не сделает твой посыл менее значимым."
    n 1k "Да! Именно!"
    n "Мне нравится, когда текст легко читается, но при этом наотмашь бьёт по чувствам."
    n 1c "Прямо как в этом стихе."
    n "Когда все вокруг умеют делать классные штуки, это так деморализует..."
    n "Так что я решила написать об этом."
    mc "Да, я понимаю."
    n 2a "Что мне ещё нравится: в простых стихах очень важно играть словами."
    n "Например, я вот задала рифму, но в последних строках она сбита."
    n "Помогает подчеркнуть акцент в финале."
    mc "И у тебя получилось..."
    mc "Кажется, в это стихотворение вложено больше, чем я понял."
    n 4y "Вот что значит быть профи!"
    n "Рада, что ты чему-то научился."
    n "Не ожидал такого от самой мелкой, да?"
    mc "Ага... как-то не ожидал."
    "Решил я напоследок подыграть ей."
    "Я не сужу людей по возрасту, но раз уж Нацуки этим гордится, кто я такой, чтобы лишать её этого."
    return

label ch2_n_end:
    call showpoem (poem_n2) from _call_showpoem_5
    n 2a "Неплохо, да?"
    mc "Вышло немного длиннее вчерашнего..."
    n 2w "Ну, тот был совсем уж короткий..."
    n "Да и вообще, это был разогрев!"
    n 2c "Надеюсь, ты не подумал, что на большее я не способна."
    mc "Нет, конечно нет..."
    n 2a "Короче, посыл здесь достаточно очевидный и однозначный."
    n "По-моему, тут нечего объяснять."
    n 2c "Сложные проблемы иногда подают через упрощённые аналоги..."
    n "Чтобы люди осознали собственную глупость."
    n 2g "Например, любой согласится, что персонаж рассказчика в этом стихе — зашоренный ублюдок..."
    mc "Среди твоих знакомых есть такие люди?"
    n 2c "Конечно. Именно так все реагируют на мою..."
    n 5w "...Неважно! Это может касаться любого!"
    n 5h "Я писала так, чтобы было проще провести аналогию..."
    n "У каждого имеется странное хобби или порочная слабость."
    n 5q "Что-то такое, что ты скрываешь от других, опасаясь, что над тобой будут смеяться или презирать, если об этом узнают."
    n 1e "...Но это же такая тупость!"
    n "Какая разница, кто чем увлекается, если это делает их счастливыми и не вредит окружающим?"
    n 1q "Я считаю, что людям очень не хватает терпимости к нестандартным увлечениям..."
    if y_readpoem and (y_poemappeal[0] >= 0 or y_poemappeal[1] >= 0):
        mc "Ха-х, забавно..."
        mc "Юри сегодня написала примерно о том же."
        n 1h "Чего?"
        n "Ты сказал, \"Юри\"?"
        mc "Ага..."
        mc "Она объяснила, что стих был о её необычном хобби."
